<!DOCTYPE intellij-compat PUBLIC "Plugin/DTD"
        "https://raw.githubusercontent.com/JetBrains/intellij-scala/idea212.x/scala/scala-impl/src/org/jetbrains/plugins/scala/components/libextensions/intellij-compat.dtd">

<intellij-compat>
    <name>Functional Mock support</name>
    <description>This provides type inference for Message.Builder macro</description>
    <version>0.0.1-SNAPSHOT</version>
    <vendor>qualiton.org</vendor>
    <ideaVersion since-build="2018.1.0" until-build="2022.0.0">
        <extension interface="org.jetbrains.plugins.scala.lang.macros.evaluator.ScalaMacroTypeable"
                   implementation="org.qualiton.mock.MacroFooableSupport">
            <description>Support for genFoo macro</description>
        </extension>
        <extension interface="org.jetbrains.plugins.scala.lang.macros.evaluator.ScalaMacroExpandable"
                   implementation="org.qualiton.mock.MacroDynamicSupport">
            <description>Support for dynamic apply macro</description>
        </extension>
        <extension interface="org.jetbrains.plugins.scala.lang.psi.impl.toplevel.typedef.SyntheticMembersInjector"
                   implementation="org.qualiton.mock.MySampleInjector">
            <description>Sample method injector</description>
        </extension>
    </ideaVersion>
</intellij-compat>
